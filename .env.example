# Ã†SI NEXUS - Environment Variables Template
# Copy this to .env and fill in your values

# Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Secret (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=your_jwt_secret_here

# Master Key Hash (SHA256 of your private key)
MASTER_KEY_HASH=your_master_key_hash_here

# Proton Drive Token (optional)
PROTON_DRIVE_TOKEN=your_proton_token_here

# Allowed Origins (for CORS)
ALLOWED_ORIGINS=https://your-site.netlify.app,http://localhost:3000

# Server Port
PORT=3000
